  "private": false,
  "scripts": {
    "n": "yarn ts-node --transpile-only -r dotenv/config",
    "db:migrate": "yarn knex migrate:latest",
    "db:reset": "yarn n scripts/dbReset.ts; yarn db:migrate",
    "docker": "docker-compose up -d",
    "develop:gql": "yarn n src/gql/index.ts",
    "develop:frontend": "yarn webpack serve -c src/web/webpack.config.ts",
    "knex": "yarn n node_modules/.bin/knex",
    "start": "yarn concurrently npm:develop:frontend",
    "webpack": "yarn n node_modules/.bin/webpack"
  },
  "dependencies": {
    "apollo-server": "^2.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "html-webpack-plugin": "^5.0.0",
    "knex": "^0.21.17",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "ulid": "^2.3.0",
    "webpack": "^5.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
